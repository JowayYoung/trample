{
	"name": "trample",
	"version": "1.0.0",
	"description": "trample",
	"keywords": [],
	"homepage": "https://github.com/JowayYoung",
	"license": "MIT",
	"main": "src/index.js",
	"author": {
		"name": "Joway Young",
		"phone": "18819249731",
		"email": "young.joway@aliyun.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JowayYoung/trample.git"
	},
	"scripts": {
		"start": "parcel doc/index.html --open",
		"remove": "rimraf node_modules"
	},
	"dependencies": {
		"jsonp-fallback": "1.1.2",
		"md5": "2.2.1"
	},
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-plugin-transform-class-properties": "6.24.1",
		"babel-plugin-transform-decorators-legacy": "1.3.5",
		"babel-plugin-transform-object-rest-spread": "6.26.0",
		"babel-polyfill": "6.26.0",
		"babel-preset-env": "1.7.0",
		"eslint": "5.1.0",
		"eslint-config-standard": "11.0.0",
		"eslint-plugin-import": "2.13.0",
		"eslint-plugin-node": "6.0.1",
		"eslint-plugin-promise": "3.8.0",
		"eslint-plugin-standard": "3.1.0",
		"node-sass": "4.9.2",
		"parcel-bundler": "1.9.6",
		"postcss-cssnext": "^3.1.0",
		"rimraf": "2.6.2"
	},
	"engines": {
		"node": ">= 8.0.0",
		"npm": ">= 5.0.0"
	},
	"browserslist": [
		"last 10 Chrome versions",
		"last 10 Firefox versions",
		"last 10 Opera versions",
		"Explorer >= 11",
		"Safari >= 8",
		"Android >= 4.4.4",
		"iOS >= 8"
	],
	"postcss": {
		"plugins": {
			"postcss-cssnext": true
		}
	},
	"babel": {
		"plugins": [
			"transform-class-properties",
			"transform-decorators-legacy",
			"transform-object-rest-spread"
		],
		"presets": [
			"env"
		]
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"commonjs": true,
			"es6": true,
			"node": true
		},
		"extends": "standard",
		"parserOptions": {
			"ecmaVersion": 9,
			"sourceType": "module"
		},
		"root": true,
		"rules": {
			"eol-last": [
				"error",
				"never"
			],
			"handle-callback-err": [
				"error",
				"error"
			],
			"indent": [
				"error",
				"tab"
			],
			"no-console": [
				"off"
			],
			"no-new": [
				"off"
			],
			"no-tabs": [
				"off"
			],
			"no-unreachable": [
				"off"
			],
			"object-curly-spacing": [
				"error",
				"always"
			],
			"operator-linebreak": [
				"error",
				"before"
			],
			"quotes": [
				"error",
				"double"
			],
			"semi": [
				"error",
				"always"
			],
			"space-before-function-paren": [
				"error",
				"never"
			]
		}
	}
}