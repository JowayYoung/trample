{
	"name": "trample",
	"version": "0.0.6",
	"description": "A front-end Web/Node general utils library",
	"keywords": [
		"trample",
		"trample.js",
		"js",
		"javascript",
		"tool",
		"util",
		"common",
		"web",
		"node",
		"date",
		"function",
		"number",
		"regexp",
		"string",
		"type",
		"cookie",
		"dom",
		"url",
		"fs",
		"process"
	],
	"author": {
		"name": "Joway Young",
		"phone": "18819249731",
		"email": "young.joway@aliyun.com"
	},
	"homepage": "https://github.com/JowayYoung",
	"repository": {
		"type": "git",
		"url": "https://github.com/JowayYoung/trample.git"
	},
	"license": "MIT",
	"main": "web.js",
	"scripts": {
		"build": "babel-node build/index.js",
		"kill": "tskill node",
		"remove": "rimraf package-lock.json yarn.lock node_modules common node web node.js node.es5.js web.js web.es5.js",
		"start": "nodemon src/index.js -x babel-node",
		"test:b": "cd test/web && bruce b",
		"test:n": "nodemon test/node/index.js -x babel-node"
	},
	"dependencies": {
		"copy-dir": "1.2.0",
		"dayjs": "1.8.20",
		"make-dir": "3.0.0",
		"rimraf": "3.0.1"
	},
	"devDependencies": {
		"@babel/cli": "7.8.4",
		"@babel/core": "7.8.4",
		"@babel/node": "7.8.4",
		"@babel/plugin-proposal-class-properties": "7.8.3",
		"@babel/plugin-proposal-decorators": "7.8.3",
		"@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
		"@babel/plugin-proposal-optional-chaining": "7.8.3",
		"@babel/plugin-syntax-dynamic-import": "7.8.3",
		"@babel/plugin-transform-runtime": "7.8.3",
		"@babel/preset-env": "7.8.4",
		"@babel/runtime": "7.8.4",
		"babel-loader": "8.0.6",
		"corejs": "1.0.0",
		"hard-source-webpack-plugin": "0.13.1",
		"nodemon": "2.0.2",
		"regenerator-runtime": "0.13.3",
		"webpack": "4.41.5",
		"webpack-cli": "3.3.10",
		"webpack-merge": "4.2.2",
		"webpackbar": "4.0.0"
	},
	"engines": {
		"node": ">= 8.0.0",
		"npm": ">= 5.0.0"
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"corejs": 3,
					"targets": {
						"node": "10.0.0"
					},
					"useBuiltIns": "usage"
				}
			]
		]
	},
	"nodemonConfig": {
		"env": {
			"NODE_ENV": "development"
		},
		"execMap": {
			"js": "node --harmony"
		},
		"ext": "js json",
		"ignore": [
			"dist/"
		],
		"watch": [
			"src/",
			"test/"
		]
	}
}